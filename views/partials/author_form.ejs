<h1><%= title %></h1>
<form method="POST" action="">
  <div class="form-group">
    <label for="first_name">First Name: </label>
    <input
      id="first_name"
      class="form-control"
      type="text"
      placeholder="First name"
      name="first_name"
      required="true"
    />
    <label for="family_name">Family Name: </label>
    <input
      id="family_name"
      class="form-control"
      type="text"
      placeholder="Family name"
      name="family_name"
      required="true"
    />
  </div>
  <div>
    <label for="date_of_birth">Date of birth: </label>
    <input
      id="date_of_birth"
      class="form-control"
      type="date"
      name="date_of_birth"
      required="true"
    />
    <label for="date_of_death">Date of death: </label>
    <input
      id="date_of_death"
      class="form-control"
      type="date"
      name="date_of_death"
      required="true"
    />
  </div>
  <button class="btn btn-primary" type="submit">Submit</button>
</form>
